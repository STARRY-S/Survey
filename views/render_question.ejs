
<% for (let i = 1; i < obj_list.length; ++i ) { %>
  <% let obj = obj_list[i] %>
  <% if (obj.type === 'input') { %>
    <%= obj.title %>： <span style="color: red;">*</span><br>
    <input type="text" name="question_<%= i %>" required><br><br>
  <% } %>

  <% if (obj.type === 'select') { %>
    <%= obj.title %>： <span style="color: red;">*</span><br>
    <select name="question_<%= i %>">
      <% for (let j = 0; j < obj.q_list.length; ++j) { %>
        <option value="<%= obj.q_list[j] %>"><%= obj.q_list[j] %></option>
      <% } %>
    </select><br><br/>
  <% } %>

  <% if (obj.type === 'multiselect') { %>
    <%= obj.title %>： <span style="color: red;">*</span><br>
    <fieldset>
      <% for (let j = 1; j <= obj.q_list.length; ++j) { %>
        <label class="container"><%= obj.q_list[j-1] %>
          <input type="checkbox" name="<%= `cb_${i}_${j}` %>" >
          <span class="checkmark"></span>
        </label>
      <% } %>
    </fieldset><br>
  <% } %>

<% } %>
<% if (typeof obj_list[0] !== "undefined" 
  && typeof obj_list[0].enable_end_time !== "undefined") 
{ %>
  <% if (obj_list[0].enable_end_time 
    && obj_list[0].end_time !== "1900-01-01 00:00:00") 
  { %> 
  <p style="color: red;">问卷截止时间为 <%= obj_list[0].end_time %> </p>
  <% } %>
<% } %>  